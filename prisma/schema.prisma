generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  code     String   @id
  password String
  name     String
  sex      String
  maritalStatus String
  email    String   @unique
  charge   String
  birthday String
  year     String
  weddingAnniversary String?
}

model scale_users {
  code     String   @id
  password String
  name     String
  email    String   @unique
  departament String  
}

model birthdays {
  code     String @id
  name     String
  birthday String
  year     String
}

model wedding_aniversary {
  code     String @id
  name     String
  birthday String
  year     String
}

model wedding {
  year     String @id
  description String
}